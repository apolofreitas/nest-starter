{
  "name": "nest-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local:dev": "APP_SERVER=local nest start --watch",
    "start:stag:dev": "APP_SERVER=staging nest start --watch",
    "start:prod:dev": "APP_SERVER=production nest start --watch",
    "start:debug": "APP_SERVER=local nest start --debug --watch",
    "start:stag": "APP_SERVER=staging node dist/main",
    "start:prod": "APP_SERVER=production node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/common": "7.4.4",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "7.4.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.1.2",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/swagger": "^4.7.5",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "mongoose": "5.10.10",
    "nanoid": "^3.1.20",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "swagger-ui-express": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.2.1",
    "@nestjs/testing": "7.4.4",
    "@types/express": "4.17.8",
    "@types/mongoose": "^5.10.2",
    "@types/node": "7.10.9",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.2",
    "prettier": "2.1.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.7",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.3"
  }
}
